{
  "name": "@creditas/person-classification",
  "version": "0.0.0-development",
  "repository": "git@github.com:Creditas/person-classification-frontend-sdk.git",
  "author": "Guilherme Mangabeira Gregio <guilherme.gregio@creditas.com.br>",
  "license": "UNLICENSED",
  "scripts": {
    "build": "vite build",
    "semantic-release": "semantic-release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-types": "tsc -p tsconfig.prod.json ",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --colors",
    "test:ci": "cross-env CI=1 yarn test --no-watch --ci --runInBand",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "types": "./dist/main.d.ts",
  "typings": "dist/main.d.ts",
  "main": "./dist/person-classification.umd.js",
  "module": "./dist/person-classification.es.js",
  "exports": {
    ".": {
      "import": "./dist/person-classification.es.js",
      "require": "./dist/person-classification.umd.js"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@swc/core": "^1.2.162",
    "@swc/jest": "^0.2.20",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/ramda": "^0.28.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-dts": "^1.0.1"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "ramda": "^0.28.0"
  }
}
